{
  "name": "statistic-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn tailwindcss && vite build",
    "build:netlify": "yarn tailwindcss && vite build && echo [[redirect]] > ./dist/netlify.toml && echo from = '/*' >> ./dist/netlify.toml && echo to = '/index.html' >> ./dist/netlify.toml && echo status = 200 >> ./dist/netlify.toml",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tailwindcss": "tailwindcss -i ./src/index.css -o ./dist/output.css",
    "tailwindcss:watch": "tailwindcss -i ./src/index.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "chart.js": "^4.3.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "tailwindcss": "^3.3.2",
    "vite": "^4.4.0"
  }
}
